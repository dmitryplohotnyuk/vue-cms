{
    "name": "kinchaku-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "dev-build": "vue-cli-service build --mode development",
        "lint": "vue-cli-service lint",
        "new": "hygen new",
        "test:e2e": "vue-cli-service test:e2e"
    },
    "dependencies": {
        "@tinymce/tinymce-vue": "^2.0.0",
        "axios": "^0.18.0",
        "babel-polyfill": "^6.26.0",
        "currency.js": "^1.2.1",
        "date-fns": "^1.29.0",
        "japan-postal-code": "^0.2.0",
        "nprogress": "0.2.0",
        "numbro": "^2.1.0",
        "tinymce": "^5.0.0",
        "typeface-roboto": "^0.0.54",
        "typeface-roboto-mono": "^0.0.54",
        "vue": "^2.6.0",
        "vue-color": "^2.7.0",
        "vue-echarts": "^3.1.3",
        "vue-meta": "^1.5.0",
        "vue-router": "^3.0.1",
        "vue-script2": "^2.0.3",
        "vue-sticky-directive": "^0.0.8",
        "vue-stripe-elements-plus": "^0.2.9",
        "vuency": "^0.1.7",
        "vuetify": "^1.5.5",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.0",
        "@vue/cli-plugin-e2e-cypress": "^3.1.2",
        "@vue/cli-plugin-eslint": "^3.0.0",
        "@vue/cli-service": "^3.0.0",
        "@vue/eslint-config-prettier": "^3.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.0.0",
        "eslint-plugin-vue": "^5.0.0-beta.4",
        "hygen": "^1.5.x",
        "lodash.camelcase": "^4.3.0",
        "node-sass": "^4.9.0",
        "normalize.css": "8.0.0",
        "sass-loader": "^7.0.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "test-data-bot": "^0.6.1",
        "vue-template-compiler": "^2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/base",
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "rules": {
            "vue/no-unused-vars": "warn"
        },
        "parserOptions": {
            "parser": "babel-eslint",
            "allowImportExportEverywhere": true
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "vue": {
        "devServer": {
            "useLocalIp": true,
            "bonjour": false,
            "host": "localhost",
            "disableHostCheck": true
        },
        "configureWebpack": {
            "output": {
                "filename": "[name].[hash].js"
            }
        },
        "transpileDependencies": [
            "resize-detector"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
